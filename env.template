# TickTick MCP Server Environment Variables
# Copy this file to .env and fill in your values (optional)

# TickTick Credentials (optional - can be set via CLI authentication)
# These will be automatically populated after running: uv run ticktick-mcp auth
TICKTICK_USERNAME=your_username_here
TICKTICK_PASSWORD=your_password_here
TICKTICK_AUTHENTICATED=false

# Note: Credentials are automatically saved to ~/.ticktick-mcp/credentials.json
# after successful authentication via CLI
